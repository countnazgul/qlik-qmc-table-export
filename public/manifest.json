{
    "name": "Export Qlik Sense QMC tables as CSV",
    "description": "Extension to export Qlik QMC tables to CSV",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*/qmc/*",
                "https://*/*/qmc/*"
            ],
            "js": [
                "build/content_script.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}